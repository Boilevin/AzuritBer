
(rules PCB charge_pcb3
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 212)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 3.0)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.5)
    )
  )
  (rule
    (width 254.0)
    (clear 254.2)
    (clear 127.0 (type smd_to_turn_gap))
    (clear 550.2 (type default_+24V))
    (clear 63.6 (type smd_smd))
    (clear 550.2 (type smd_+24V))
    (clear 550.2 (type "kicad_default"_+24V))
    (clear 550.2 (type +24V_+24V))
    (clear 550.2 (type +24V_+3V))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-+24V" "Via[0-1]_800:400_um" +24V
  )
  (via 
    "Via[0-1]_800:400_um-+3V" "Via[0-1]_800:400_um" +3V
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    +24V "Via[0-1]_800:400_um-+24V"
  )
  (via_rule
    +3V "Via[0-1]_800:400_um-+3V"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 254.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND "Net-(BT1-Pad1)" "Net-(C1-Pad1)" "Net-(D1-Pad1)" "Net-(D2-Pad2)" "Net-(D3-Pad1)" "Net-(D4-Pad1)" "Net-(D5-Pad2)"
    "Net-(F2-Pad1)" pinVoltageMeasurement pinChargeEnable pinChargeCurrent pinChargeVoltage pinBatterySwitch pinBatteryVoltage "Net-(J2-Pad1)"
    "Net-(Q1-Pad1)" "Net-(Q1-Pad2)" "Net-(Q3-Pad1)" "Net-(Q3-Pad2)" +3V3 "Net-(D6-Pad2)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 254.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class +24V
    (clearance_class +24V)
    (via_rule +24V)
    (rule
      (width 1500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class +3V
    (clearance_class +3V)
    (via_rule +3V)
    (rule
      (width 500.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)